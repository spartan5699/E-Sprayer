<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Sensor_data"
    android:background="@drawable/back3">


    <Button
        android:id="@+id/prediction"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"

        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:layout_marginBottom="75dp"
        android:background="@drawable/custombtn2"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="25"
        android:soundEffectsEnabled="true"
        android:text="Prediction"
        android:textColor="#030303"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/pressdata"
        android:layout_width="190dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:layout_marginBottom="204dp"
        android:textColor="#050505"
        android:textSize="35sp"
        app:fontFamily="@font/brawler"
        tools:text="987.22" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="283dp"
        android:text="Pressure"
        android:textColor="#FD050505"
        android:textSize="30sp"
        android:textStyle="bold"
        app:fontFamily="@font/brawler" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="236dp"
        android:layout_marginRight="236dp"
        android:layout_marginBottom="200dp"
        app:srcCompat="@drawable/pressure" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="654dp"
        android:text="Current Readings"
        android:textAlignment="center"
        android:textColor="#130808"
        android:textSize="45sp"
        android:textStyle="bold"
        app:fontFamily="@font/cinzel_decorative_bold"
        android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-2dp"
        android:layout_marginRight="-2dp"
        android:layout_marginBottom="575dp"
        android:text="@string/temperature"
        android:textColor="#FD050505"
        android:textSize="30sp"
        android:textStyle="bold"
        app:fontFamily="@font/brawler" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="427dp"
        android:text="Humidity"
        android:textColor="#FD050505"
        android:textSize="30sp"
        android:textStyle="bold"
        app:fontFamily="@font/brawler" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="236dp"
        android:layout_marginRight="236dp"
        android:layout_marginBottom="493dp"
        app:srcCompat="@drawable/temperature" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="236dp"
        android:layout_marginRight="236dp"
        android:layout_marginBottom="347dp"
        app:srcCompat="@drawable/humidity" />

    <TextView
        android:id="@+id/tempdata"
        android:layout_width="190dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:layout_marginBottom="494dp"
        android:textColor="#0C0C0C"
        android:textSize="35sp"
        app:fontFamily="@font/brawler" />

    <TextView
        android:id="@+id/humidata"
        android:layout_width="190dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:layout_marginBottom="348dp"
        android:textColor="#050505"
        android:textSize="35sp"
        app:fontFamily="@font/brawler" />

    <Button
        android:id="@+id/forecast"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"

        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="212dp"
        android:layout_marginRight="212dp"
        android:layout_marginBottom="73dp"
        android:background="@drawable/custombtn2"
        android:shadowColor="#A8A8A8"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="25"
        android:soundEffectsEnabled="true"
        android:text="Forecast"
        android:textColor="#030303"
        android:textSize="25sp" />
</RelativeLayout>